<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import java.net.URL?>
<GridPane alignment="CENTER" hgap="10.0" vgap="10.0" xmlns:fx="http://javafx.com/fxml"
          fx:controller="edu.rf.ui.MainController">
    <children>
        <Button id="" fx:id="someFucker" mnemonicParsing="false" onAction="#handleClickAction" style=""
                styleClass="green" text="Is It Working?" GridPane.columnIndex="0" GridPane.rowIndex="0"/>
